{
  "name": "my-iso",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "db:push": "prisma generate && prisma db push",
    "db:studio": "prisma studio",
    "dev": "next dev",
    "build:cf": "npx opennextjs-cloudflare build",
    "deploy": "npm run build:cf && wrangler deploy",
    "dev:wrangler": "npm run build:cf && wrangler dev",
    "lint": "next lint",
    "postinstall": "cross-env PRISMA_ENGINES_CHECKSUM_IGNORE_MISSING=1 prisma generate || echo 'Prisma generation failed - this is a known CDN issue. Build may continue if client was previously generated.'",
    "start": "next start",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:reset": "prisma db push --force-reset && npx tsx prisma/seed.ts",
    "db:seed:staging": "wrangler d1 execute my-iso-db-staging --remote --env staging --file=prisma/seed-staging.sql"
  },
  "dependencies": {
